.ourpartner
  width: 100%
  margin: 0
  padding-top: 100px
  padding-bottom: 70px
  .partner-list
    padding-top: 90px
    margin-bottom: 20px
    display: block
    display: flex
    flex: 1 1 auto
    flex-wrap: wrap
    justify-content: space-between
    align-items: center
  &__item
    lost-column: 1/4 4 10px
    display: block
    margin-bottom: 10px
    background-color: #ffffff
    .partner-box
      margin: 0
      padding: 0
      position: relative
